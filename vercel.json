{
  "functions": {
    "api/python/*.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    },
    "api/php/*.php": {
      "runtime": "php@8.0.0"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/api/python/(.*)",
      "dest": "/api/python/index.py?path=$1"
    },
    {
      "src": "/api/php/(.*)",
      "dest": "/api/php/index.php?path=$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/public/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/**/*.php",
      "use": "@vercel/php"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ]
}